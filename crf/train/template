# advantaged features
# pre and ppre data
U60:%x[-2,0]
U61:%x[-1,0]
U62:%x[1,0]
U63:%x[2,0]

# et, al ///title
U63:%x[-2,0]/%x[-1,0]

# jump out quot
U65:%x[-1,18]

# jump out some special flag?
U66:%x[-1,3]

# stop flag ?
U67:%x[-1,40]

# stop & quot
U68:%x[-1,40]/%x[0,18]

# last len and last stop
U69:%x[-1,40]/%x[-1,4]

#Axxx Axxx CD
U6A:%x[0,8]/%x[1,8]/%x[2,8]

# eds flag
U70:%x[0,41]/%x[0,19]

# combined featues
# delimiters () ? "" ?  etc.
UA0:%x[0,1]/%x[0,3]
UA1:%x[0,2]/%x[0,3]
UA2:%x[-1,1]/%x[-1,3]
UA3:%x[-2,1]/%x[-1,3]
UA4:%x[-2,2]/%x[-2,3]

# location like ? len(4)/type(8)
UA5:%x[0,4]/%x[0,8]

# volumn 1(2)
UA6:%x[-1,6]/%x[0,6]/%x[0,1]/%x[0,3]

# vol/volumn . 123 n/number/No. 123
UA7:%x[0,11]/%x[0,3]/%x[1,6]/%x[2,11]/%x[2,3]/%x[3,6]

# vol . 123
UA8:%x[0,11]/%x[0,3]/%x[1,6]

# volumn :  23,231
UA9:%x[0,6]/%x[0,3]/%x[1,6]

# date: (1991)
UAA:%x[0,2]/%x[0,9]/%x[0,3]


# Technical Report xxx xx
UAB:%x[0,0]/%x[1,0]/%x[0,3]


# In proceeding of 
UAC:%x[0,0]/%x[0,3]/%x[1,0]/%x[1,3]/%x[2,0]
#UAC:%x[0,15]/%x[1,15]
# New York : MA  todo: check
UAD:%x[0,0]/%x[0,3]/%x[1,0]/%x[1,3]/%x[2,0]


# Xxxx Xxxx : XX
UAF:%x[0,8]/%x[0,3]/%x[1,8]/%x[1,3]/%x[2,8]
UB0:%x[1,4]/%x[1,8]
UB1:%x[1,4]/%x[1,8]/%x[0,8]

# Proceedings of ..
UB2:%x[0,0]/%x[0,3]/%x[1,0]


# 27: isMonthInDict / year like
UB3:%x[0,27]/%x[0,9]

# 28 : a,an,the, / delimiter
UB4:%x[0,28]/%x[0,3]

# 29 : dept. xx department of ...
UB5:%x[0,29]/%x[0,3]



//32 : http effect
UB8:%x[-1,32]

# http / after delimiter
UB9:%x[0,32]/[0,3]


# In next is name like

# stop flag
UC0:%x[-1,18]

# oct . 80 2013
UC1:%x[-1,43]/%x[0,6]/%x[1,43]


# technical report
UC2:%x[0,41]/%x[1,41]

# connected spec
UC3:%x[0,15]/%x[1,15]

# sth before '('?
UD0:%x[0,34]/%x[0,2]

# (Oct xxx?
UD1:%x[0,43]/%x[0,34]

